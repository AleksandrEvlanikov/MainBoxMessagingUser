@model List<User> 

<h2>Список пользователей</h2>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Password</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.Password</td>
                <td>
                    <!-- Add any other user details you want to display -->
                    <a asp-action="DeleteUser" asp-route-id="@user.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
